<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://amjad.com/bundles"
           targetNamespace="http://amjad.com/bundles" elementFormDefault="qualified">

    <xs:element name="addBundleRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="bundle" type="tns:bundle"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="addBundleResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="bundle" type="tns:bundle"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="bundle">
        <xs:sequence>
            <xs:element name="id" type="xs:int" />
            <xs:element name="size" type="xs:double" />
            <xs:element name="price" type="xs:double" />
            <xs:element name="activate_date" type="xs:string" />
            <xs:element name="end_date" type="xs:string" />
        </xs:sequence>
    </xs:complexType>

</xs:schema>